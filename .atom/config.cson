"*":
  "atom-beautify":
    html:
      default_beautifier: "Pretty Diff"
      indent_size: 2
      preserve_newlines: false
  "atom-ide-ui":
    "atom-ide-code-format":
      formatOnSave: true
  core:
    disabledPackages: [
      "github"
      "spell-check"
    ]
    telemetryConsent: "limited"
    titleBar: "hidden"
  editor:
    fontFamily: "\"Fira Code\""
    fontSize: 16
    invisibles: {}
    showInvisibles: true
    softWrap: true
  "exception-reporting":
    userId: "e55cc812-438c-4cfa-a9b8-a78cdae94846"
  "file-icons":
    coloured: false
    onChanges: true
  "fuzzy-finder":
    ignoredNames: [
      "node_modules"
    ]
    scoringSystem: "fast"
    useRipGrep: true
  "language-babel":
    taggedTemplateGrammar: [
      "\"(?<=<style jsx>{)|(?<=<style jsx global>{)\":source.css.styled"
    ]
  linter:
    disabledProviders: [
      "RuboCop"
    ]
  "linter-ui-default":
    showPanel: true
  "no-title-bar":
    enableLiberalWindowDragging: false
  "one-dark-ui": {}
  "one-light-ui": {}
  "prettier-atom":
    formatOnSaveOptions:
      showInStatusBar: true
    prettierOptions:
      parser: "flow"
  "spell-check":
    locales: [
      "en-GB"
    ]
  tabs:
    enableVcsColoring: true
  "tree-view":
    hideVcsIgnoredFiles: true
  welcome:
    showOnStartup: false
